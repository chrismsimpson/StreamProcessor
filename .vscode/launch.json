{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "run",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/Sources/bin/Debug/net6.0/StreamProcessor.dll",
            "args": [
                // "--read-file", "./Samples/singleTransaction.json"
                "--read-file", "./Samples/transactions.json"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "stopAtEntry": false,
            "logging": {
                "moduleLoad": false,
                "engineLogging": false,
                "elapsedTiming": false,
                "exceptions": true,
                "processExit": true,
                "programOutput": true,
                "threadExit": false
            },
            "requireExactSource": true
        }
    ]
}